{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "env": {
        "NEXT_PUBLIC_API_BASE_URL": "https://api.hapinas.net/api/v1"
    },
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "https://api.hapinas.net"
                }
            ]
        }
    ]
}